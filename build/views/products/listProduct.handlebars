<table class="table w-100">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Nombre</th>
        <th scope="col">Marca</th>
        <th scope="col">No. Stock</th>
        <th scope="col">Cantidad</th>
    </tr>
    </thead>
    <tbody>
    {{#each data}}
        <tr>
            <th scope="row">{{this.idProducto}}</th>
            <td>{{this.nombreProducto}}</td>
            <td>{{this.marcaProducto}}</td>
            <td>{{this.cantidadStock}}</td>
            <td>{{this.cantidadTipo}}{{this.tipo}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>